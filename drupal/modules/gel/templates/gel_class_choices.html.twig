{#
/**
 * @file gel_class_choices.html.twig
 * Default theme implementation to present Gel class choices data.
 *
 * This template is used when viewing Gel class choices pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_gel_class_choices()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('gel_class_choices') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
