{#
/**
 * @file gel_school.html.twig
 * Default theme implementation to present Gel_school data.
 *
 * This template is used when viewing Gel_school pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_gel_school()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('gel_school') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
