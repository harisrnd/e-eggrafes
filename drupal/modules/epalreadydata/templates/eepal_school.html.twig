{#
/**
 * @file eepal_school.html.twig
 * Default theme implementation to present Eepal school data.
 *
 * This template is used when viewing Eepal school pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_eepal_school()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('eepal_school') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
